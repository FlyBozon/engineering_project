  "training_params": {
    "default": {
      "batch_size": 16,
      "epochs": 100,
      "learning_rate": 0.001,
      "optimizer": "adam",
      "loss_function": "sparse_categorical_crossentropy",
      "metrics": ["accuracy", "mean_iou"]
    },
    "satellite": {
      "batch_size": 32,
      "patch_size": 256,
      "learning_rate": 0.0005
    },
    "drone": {
      "batch_size": 16,
      "patch_size": 512,
      "learning_rate": 0.001
    }
