{
  "datasets": {
    "landcover.ai": {
      "name": "LandCover.ai",
      "description": "High-resolution land cover classification dataset for Poland",
      "type": "satellite",
      "classes": {
        "num_classes": 5,
        "class_names": ["unlabeled", "buildings", "woodlands", "water", "roads"],
        "class_ids": [0, 1, 2, 3, 4],
        "class_colors": {
          "unlabeled": [0, 0, 0],
          "buildings": [128, 0, 0],
          "woodlands": [0, 128, 0], 
          "water": [0, 0, 128],
          "roads": [128, 128, 128]
        },
        "ignore_class": 0
      },
      "data_format": {
        "image_format": "tif",
        "mask_format": "tif", 
        "mask_type": "grayscale_indexed",
        "image_channels": 3,
        "bit_depth": 8
      },
      "preprocessing": {
        "patch_size": 256,
        "overlap": 64,
        "normalization": "minmax",
        "resize_needed": false
      },
      "paths": {
        "dataset_dir": "landcover.ai.v1" 
      }
    },
    
    "deepglobe": {
      "name": "DeepGlobe",
      "description": "RGB satellite imagery for land cover classification",
      "type": "satellite",
      "classes": {
        "num_classes": 7,
