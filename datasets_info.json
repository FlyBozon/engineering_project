        "class_colors": {
          "unknown": [0, 0, 0],
          "urban": [0, 255, 255],
          "agriculture": [255, 255, 0],
          "rangeland": [255, 0, 255],
          "forest": [0, 255, 0],
          "water": [0, 0, 255],
          "barren": [255, 255, 255]
        },
        "ignore_class": 0
      },
      "data_format": {
        "image_format": "jpg",
        "mask_format": "png",
        "mask_type": "rgb_color_coded",
        "image_channels": 3,
        "bit_depth": 8
      },
      "preprocessing": {
        "patch_size": 256,
        "overlap": 32,
        "normalization": "standardization",
        "resize_needed": true,
        "target_size": [512, 512]
      },
      "paths": {
        "dataset_dir": "deepglobe"
      }
    },
